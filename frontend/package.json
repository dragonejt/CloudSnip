{
	"name": "vsfriends",
	"displayName": "VSFriends",
	"description": "A friend finder and adder for VSCode",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vsfriends.login",
		"onCommand:vsfriends.logout",
		"onUri",
		"onView:friends"
	],
	"main": "./extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "vsfriends",
					"title": "VSFriends",
					"icon": ".//lib//img//organization.svg"
				}
			]
		},
		"views": {
			"vsfriends": [
				{
					"id": "vsfriends.friends",
					"name": "",
					"contextualTitle": ""
				}
			]
		},
		"commands": [
			{
				"command": "vsfriends.login",
				"title": "VSFriends: Sign In with GitHub"
			},
			{
				"command": "vsfriends.logout",
				"title": "VSFriends: Sign Out"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	},
	"dependencies": {
		"dotenv": "^8.2.0"
	}
}
